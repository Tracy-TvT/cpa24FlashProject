<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxapplication1.DashboardController">
    <children>
        <TabPane fx:id="tabPane" tabClosingPolicy="UNAVAILABLE" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <tabs>
                <Tab text="Expenses and Income">
                    <content>
                        <fx:include source="expenses-income.fxml" />
                    </content>
                </Tab>
                <Tab text="Monthly Budget">
                    <content>
                        <fx:include source="monthly-budget.fxml" />
                    </content>
                </Tab>
                <Tab text="Saving Goals">
                    <content>
                        <fx:include source="saving-goals.fxml" />
                    </content>
                </Tab>
                <Tab text="View Transactions">
                    <content>
                        <fx:include source="transactions.fxml" />
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
